    Call Audio.PlayWave(SND_CLICK)
    Unload Me
End Sub

Public Sub Iniciar_Labels()

    'Iniciamos los labels con los valores de los atributos y los skills
    Dim i As Integer

    For i = 1 To NUMATRIBUTOS
        Atri(i).ForeColor = &H10B8EF
        Atri(i).FontBold = True
        Atri(i).Font = "Ms Sans serif"
        Atri(i).Caption = UserAtributos(i)
    Next

    For i = 1 To NUMSKILLS
        Skills(i).ForeColor = &H10B8EF
        Skills(i).FontBold = True
        Skills(i).Font = "Ms Sans serif"
        Skills(i).Caption = UserSkills(i)
    Next
    
    For i = 1 To 6
        Label4(i).ForeColor = &H10B8EF
        Label4(i).FontBold = True
        Label4(i).Font = "Ms Sans serif"
    Next i
    
    Label4(1).Caption = UserReputacion.AsesinoRep
    Label4(2).Caption = UserReputacion.BandidoRep
    Label4(3).Caption = UserReputacion.BurguesRep
    Label4(4).Caption = UserReputacion.LadronesRep
    Label4(5).Caption = UserReputacion.NobleRep
    Label4(6).Caption = UserReputacion.PlebeRep

    If UserReputacion.Promedio < 0 Then
        Label4(7).ForeColor = vbRed
        Label4(7).Caption = "CRIMINAL"
        Label4(i).Font = "Ms Sans serif"
        Label4(i).FontBold = True
    Else
        Label4(7).ForeColor = vbBlue
        Label4(7).Caption = "CIUDADANO"
        Label4(i).Font = "Ms Sans serif"
        Label4(i).FontBold = True
    End If
    
    For i = 1 To 33
        Label6(i).ForeColor = &H10B8EF
        Label6(i).FontBold = True
        Label6(i).Font = "Ms Sans serif"
    Next i

    With UserEstadisticas
        Label6(0).Caption = .CriminalesMatados
        Label6(1).Caption = .CiudadanosMatados
        Label6(2).Caption = .UsuariosMatados
        Label6(3).Caption = .NpcsMatados
        Label6(4).Caption = .Clase
        Label6(5).Caption = .Raza
        Label6(6).Caption = .PenaCarcel
        Label6(7).Caption = .PuntosClan
        Label6(8).Caption = .Name
        Label6(9).Caption = .Genero
        Label6(10).Caption = .PuntosRetos
        Label6(11).Caption = .PuntosTorneos
        Label6(12).Caption = .PuntosDuelos
        Label6(13).Caption = .Stats.Nivel
        Label6(14).Caption = .Stats.MinExp & "/" & .Stats.MaxExp
        Label6(15).Caption = .Stats.MinHP & "/" & .Stats.MaxHP
        Label6(16).Caption = .Stats.MinMan & "/" & .Stats.MaxMan
        Label6(17).Caption = .Stats.MinSta & "/" & .Stats.MaxSta
        Label6(18).Caption = .Stats.Oro
        Label6(19).Caption = .Stats.Banco
        Label6(20).Caption = .pos.Map
        Label6(21).Caption = .pos.PosX & "," & .pos.PosY
        Label6(22).Caption = .Stats.SkillPoins
        Label6(23).Caption = .ParticipoClan
        Label6(24).ForeColor = vbRed
        Label6(24).Caption = .AbbadonMatados
        Label6(25).ForeColor = vbBlue
        Label6(25).Caption = .CleroMatados
        Label6(26).ForeColor = "&H808080"
        Label6(26).Caption = .TinieblaMatados
        Label6(27).ForeColor = vbWhite
        Label6(27).Caption = .TemplarioMatados
        If UCase$(.Faccion.Armada) = "CLERO" Then
            Label6(28).ForeColor = vbBlue
            Label6(28).Caption = .Faccion.Armada
            Label6(30).Caption = .Faccion.Recompensas
        ElseIf UCase$(.Faccion.Armada) = "ABBADON" Then
            Label6(28).ForeColor = vbRed
            Label6(28).Caption = .Faccion.Armada
            Label6(30).Caption = .Faccion.Recompensas
        ElseIf UCase$(.Faccion.Armada) = "TINIEBLA" Then
            Label6(28).ForeColor = "&H808080"
            Label6(28).Caption = .Faccion.Armada
            Label6(30).Caption = .Faccion.Recompensas
        ElseIf UCase$(.Faccion.Armada) = "TEMPLARIO" Then
            Label6(28).ForeColor = vbWhite
            Label6(28).Caption = .Faccion.Armada
            Label6(30).Caption = .Faccion.Recompensas
        Else
            Label6(28).Caption = "Ninguna"
            Label6(30).Caption = .Faccion.Recompensas
        End If
        Label6(29).Caption = .Faccion.Reenlistado
        Label6(31).Caption = .Faccion.CiudadanosMatados
        Label6(32).Caption = .Faccion.CriminalesMatados
        Label6(33).Caption = .Faccion.FEnlistado
    End With

End Sub

